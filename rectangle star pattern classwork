Program1: Rectangle Star Pattern
import java.io.*;
import java.util.*;
class rectstarpattern
{
public static void main(String args[])
{
try
{
int i,j;
Scanner sc=new Scanner(System.in);
System.out.println("Enter the number of rows:");
int r=sc.nextInt();
System.out.println("Enter the number of columns:");
int c=sc.nextInt();
for(i=1;i<=r;i++)
{
for(j=1;j<=c;j++)
{
System.out.print("* ");
}
System.out.println();
}
}
catch(Exception e)
{
System.out.println("invalid");
}
}
}
